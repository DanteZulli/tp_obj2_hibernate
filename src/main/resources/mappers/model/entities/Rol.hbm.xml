<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.grupo25.tp_obj2_hibernate.model.entities.Rol" table="rol">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="nombre" column="nombre" type="string"/>
        
        <bag name="tecnicos" table="tecnico" inverse="true">
            <key column="rol_id"/>
            <one-to-many class="com.grupo25.tp_obj2_hibernate.model.entities.Tecnico"/>
        </bag>
    </class>
</hibernate-mapping>